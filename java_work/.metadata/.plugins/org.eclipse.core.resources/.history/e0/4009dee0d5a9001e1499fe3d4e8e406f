package ex03;

public class Ex05 {
	public static void main(String[] args) {
		System.out.print("In이 없으면 줄바꿈이 안됩니다.");
		System.out.println("=========");
		System.out.println("In이 없으면 줄바꿈이 됩니다.");
		System.out.println("=========");
		
		
		System.out.println("************");
		System.out.print("ㅁㅁㅁㅁㅁㅁ");
		System.out.print("ㅅㅅㅅㅅㅅㅅ");
		System.out.println("************");
	}

}
