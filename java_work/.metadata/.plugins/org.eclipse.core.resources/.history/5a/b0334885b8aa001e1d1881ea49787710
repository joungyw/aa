package ex03;

import java.util.Scanner;

public class Ex02 {
	 public static void main(String[] args) {
	        Scanner sc = new Scanner(System.in);
	        String str = sc.next();
	        int n = sc.nextInt();
	        
	        for (int n1 = 0; n1 <= n; n++) {
	        System.out.print(str);
	    }
	}
	}